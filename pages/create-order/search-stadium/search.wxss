.container {
    padding: 32rpx 32rpx;
}

.search-bar {
    width: 594rpx;
    height: 68rpx;
    background: rgba(243, 243, 243, 1);
    border-radius: 34rpx;
    padding: 18rpx 24rpx 18rpx 36rpx;
    display: flex;
    align-items: center;
    box-sizing: border-box;
}

.search-bar input {
    padding-left: 20rpx;
    flex: 1;
    font-size:24rpx;
    color:#333;
}

.search-bar .icon {
    width: 32rpx;
    height: 32rpx;
}

.search-bar .holder {
    font-size: 24rpx;
    color: #979797;
}


.cancel {
    color: #666666;
    font-size: 28rpx;
    align-self: center;
}

.stadium-list{
    padding:28rpx 0;
    border-bottom:2rpx solid #ededed;
}

.empty{
    min-height:800rpx;
    display:flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
    font-size:24rpx;
    color:#333;
}
.empty image{
    width:200rpx;
    height:200rpx;
    margin-bottom:20rpx;
}
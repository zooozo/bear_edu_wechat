.container{
    background-color:#F5F5F5;
    height:auto;
    padding-bottom: 138rpx;
}
.content {
    padding: 0 32rpx 80rpx;
    background-color:#fff;
}

.header-title {
    height: 400rpx;
    position: relative;
    display: flex;
    align-items: flex-end;
    padding: 0 32rpx;
    background:#fff;
}

.head-img {
    width: 100%;
    height:320rpx;
    filter: blur(4rpx);
    position: absolute;
    top: 0;
    left: 0;
    background-size: 100% 500rpx;
    background-repeat: no-repeat;
}

.userInfo {
    display: flex;
    justify-content: space-between;
    position: relative;
    z-index: 10;
    width: 100%;
}

.userInfo .name {
    font-size: 36rpx;
    color: #fff;

}

.userInfo .avatar {
    width: 144rpx;
    height: 144rpx;
    border-radius: 50%;
}

.userInfo .rate {
    margin-top: 36rpx;
    margin-bottom: 18rpx;
    color: #C6C6C6;
    font-size: 24rpx;

}

.userInfo .rate .t1 {
    color: #999999;
    margin-left: 8rpx;

}

.userInfo .rate .t2 {
    color: #F04B7E;
    margin-left: 8rpx;
}

.copyId {
    font-size: 24rpx;
    color: #999;
    display: flex;
    align-items: center;
}

.copyId view:first-child {
    margin-right: 16rpx;
}

.follow {
    color: #fff;
    font-size: 24rpx;
    border-radius: 26rpx;
    height: 52rpx;
    width: 96rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    border: 2rpx solid rgba(255, 255, 255, 1);
}

.intruduce {

    color: #666;
    font-size: 26rpx;
    padding-top: 44rpx;
    padding-bottom: 92rpx;
}

.trainer {
    color: #666;
    font-size: 24rpx;

}

.badmtn-traner {
    color: #333;
    font-size: 28rpx;
    font-weight: 700;
}

.trainer .price .txt {
    color: #FF6060;
    font-size: 28rpx;
    font-weight: 600;

}

.trainer .price .txt1 {
    color: #FF6060;
    font-size: 24rpx;

}

.skill {
    margin-top: 80rpx;
    font-size: 24rpx;
}

.skill .pic {
    width: 100%;
    height: 320rpx;
    border-radius: 10rpx;
    margin: 28rpx 0;
}

.answering {
    margin-top: 82rpx;
}

.answering .skill-list {
    margin-top: 24rpx;
    display: flex;

    flex-wrap: wrap;
}

.answering .skill-list .skill-item {
    font-size: 24rpx;
    color: #666;
    background: #F5F5F5;
    display: flex;
    align-items: center;
    justify-content: center;
    width: 216rpx;
    height: 60rpx;
    flex-shrink: 0;
    margin-bottom: 16rpx;
}

.user-order {
    display: flex;
    margin-top:52rpx;
}

.user-order .avatar{
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%;
}
.user-order .flex-center{
   color:#C6C6C6;
    font-size:24rpx;
    margin-top:10rpx;
}
.user-order .order-num{
    width:116rpx;
    height:32rpx;
    border-radius:4rpx;
    border:2rpx solid rgba(56,168,255,1);
    font-size:22rpx;
    padding:4rpx;
    font-family:PingFangSC-Regular,PingFang SC;
    font-weight:400;
    color:rgba(56,168,255,1);
    margin-left:20rpx;

}

.appraise{
    margin-top:36rpx;
    color:#666;
    font-size:28rpx;
}

.more{
    margin-top:54rpx;
    color:#999;
    font-size:24rpx;
    text-align: center;
    width:300rpx;
    height:60rpx;
    display:flex;
    align-items: center;
    justify-content: center;
    border-radius:48rpx;
    border:2rpx solid rgba(229,229,229,1);
}

.recommend{
  margin-top:16rpx;
    background:#fff;
    padding:32rpx;
}

.foot-btn {
    height: 138rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding:0 32rpx;
    position:fixed;
    bottom:0;
    left:0;
    width:100%;
    box-sizing: border-box;
    background:#fff;
}

.foot-btn .btn {

    display: flex;
    align-items: center;
    justify-content: center;
}

.foot-btn .btn1 {
    border-radius: 46rpx;
    border: 2rpx solid rgba(241, 76, 127, 1);
    color: #F14C7F;
    font-size: 32rpx;
    font-weight: 600;
    width:252rpx;
    height:90rpx;
}

.foot-btn .btn2 {
    background: linear-gradient(246deg, rgba(248, 151, 120, 1) 0%, rgba(241, 75, 127, 1) 100%);
    border-radius: 46rpx;
    color: #f8f8f8;
    font-size: 32rpx;
    width:406rpx;
    height:90rpx;
}
